<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.jm.mapper.StudentMapper">

    <select id="selectById" resultType="com.jm.pojo.Student">
        select id,name,age
        from student
        where id=#{id}
    </select>
</mapper>